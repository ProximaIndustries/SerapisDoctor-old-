<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SerapisDoctor.View.MainPage"
             Title="Serapis Doctor"     
            MasterBehavior="Popover" 
            xmlns:masterpage="clr-namespace:SerapisDoctor.View"      
            >

    <MasterDetailPage.ToolbarItems>
        <ToolbarItem Icon="CheckUpIcon.png" Command="{Binding WaitingListCommand}"/>
    </MasterDetailPage.ToolbarItems>

    <MasterDetailPage.Master>
        <masterpage:SideMenuPage Title="Menu"/>
    </MasterDetailPage.Master>
    
</MasterDetailPage>